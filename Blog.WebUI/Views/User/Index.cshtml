@model IEnumerable<ApplicationUser>
@{
    ViewData["Title"] = "Index";
}

<div class="col-sm-9">
    <table class="table">
        <tr>
            <th>User id</th>
            <th>User Name</th>
            <th>Email</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }
    </table>
</div>

